{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo":{
        "padding":{
            "top": 0
        }
    },
    "general": {
        "multithreading": true
    },
    "display":{
        "disableLineWrap": true,
        "hideCursor": true,
        "separator": " >> ",
        "percent": {
            "type": 1
        }
    },
    "modules": [
        {
            "type": "users",
            "key": " User",
            "keyColor": "blue"
        },
        "break",
        
        {
            "type": "os",
            "format": "{4} {6} ({12})",
            "key": " OS",
            "keyColor": "blue"
        },
        {
            "type": "kernel",
            "key": "├ Kernel",
            "keyColor": "blue"
        },
        {
            "type": "uptime",
            "key": "├󰔚 Uptime",
            "keyColor": "blue"
        },
        {
            "type": "shell",
            "key": "└ Shell",
            "keyColor": "blue"
        },
        
        "break",

        {
            "type": "wmtheme",
            "key":" Theme",
            "keyColor": "green"
        },
        {
            "type": "locale",
            "key": "├ Locale",
            "keyColor": "green"
        },
        {
            "type": "terminal",
            "key": "├ Terminal",
            "format": "{5}",
            "keyColor": "green"
        },
        {
            "type": "terminalfont",
            "key": "└󰛖 Terminal Font",
            "keyColor": "green"
        },
        
        "break",
        
        {
            "type": "host",
            "key": "󱩛 Host",
            "format": "{2}",
            "keyColor": "red"
        },
        {
            "type": "cpu",
            "key": "├ CPU",
            "keyColor": "red"
        },
        {
            "type": "gpu",
            "key": "├ GPU",
            "keyColor": "red"
        },
        {
            "type": "memory",
            "key": "├ Memory",
            "keyColor": "red"
        },
        {
            "type": "display",
            "key": "└󰍹 Display",
            "keyColor": "red"
        },
    ]
}